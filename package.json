{
  "name": "ecp-portal-static",
  "version": "1.0.0",
  "description": "webpact + react + redux + es6 脚手架",
  "directories": {
    "doc": "docs",
    "test": "tests"
  },
  "scripts": {
    "lint": "eslint src",
    "test": "make test",
    "start": "npm run dev:local",
    "dev:local": "cross-env NODE_ENV=development node build/webpack.dev.js --hot",
    "dev:test": "cross-env NODE_ENV=development node build/webpack.dev.js --hot --deploy",
    "release": "npm run deploy:online",
    "deploy:test": "cross-env NODE_ENV=production node build/webpack.release.js --watch --uglify --deploy=test",
    "deploy:online": "cross-env NODE_ENV=production node build/webpack.release.js --uglify --deploy=online",
    "async": "cross-env BABEL_ENV=cjs node src/modules/asnc/server.js && dev:local"
  },
  "keywords": [
    "webpack",
    "react",
    "es6",
    "babel",
    "redux",
    "boilerplate"
  ],
  "author": "ecp",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "^6.3.7",
    "babel-core": "^6.10.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-import": "^1.1.1",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.22.0",
    "cross-env": "^2.0.0",
    "css-loader": "^0.23.1",
    "eslint": "^3.1.1",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-import": "^1.11.0",
    "eslint-plugin-jsx-a11y": "^2.0.1",
    "eslint-plugin-react": "^5.2.2",
    "extract-text-webpack-plugin": "^2.0.0-rc.3",
    "ftp": "^0.3.10",
    "glob": "^7.0.5",
    "html-webpack-plugin": "^2.22.0",
    "image-webpack-loader": "^2.0.0",
    "material-ui": "^0.17.0",
    "md5": "^2.1.0",
    "node-sass": "^3.8.0",
    "postcss-loader": "^0.9.1",
    "precss": "^1.4.0",
    "react-hot-loader": "^1.3.0",
    "react-tap-event-plugin": "^2.0.1",
    "redux-logger": "^3.0.1",
    "sass-loader": "^4.0.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^2.2.1",
    "webpack-dev-middleware": "^1.10.2",
    "webpack-dev-server": "^1.14.1",
    "webpack-hot-middleware": "^2.18.0",
    "webpack-md5-hash": "0.0.5"
  },
  "dependencies": {
    "antd": "^2.9.0",
    "isomorphic-fetch": "^2.2.1",
    "prop-types": "^15.5.7",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.2",
    "react-router": "^3.0.2",
    "redux": "^3.6.0",
    "redux-saga": "^0.14.8",
    "redux-thunk": "^2.2.0"
  }
}
